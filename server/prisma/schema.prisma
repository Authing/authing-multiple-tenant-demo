generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DEMO_DATABASE_URL")
}

model User {
  id Int @id
  email String? @unique
  phone String? @unique
  name String?
  username String? @unique
  photo String?
  authingToken String?
  authingId String @unique
}
